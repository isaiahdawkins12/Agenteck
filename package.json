{
  "name": "agenteck",
  "version": "0.1.0",
  "description": "A highly customizable multi-agent terminal orchestrator for managing CLI agents like Claude Code",
  "type": "module",
  "main": "dist/main/main.js",
  "bin": {
    "agenteck": "dist/cli/cli.js"
  },
  "author": "Agenteck Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/agenteck.git"
  },
  "keywords": [
    "terminal",
    "agent",
    "claude",
    "ai",
    "orchestrator",
    "electron",
    "cli"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "start": "electron .",
    "preview": "npm run build && electron .",
    "cli": "node dist/cli/cli.js",
    "cli:start": "node dist/cli/cli.js start",
    "cli:stop": "node dist/cli/cli.js stop",
    "cli:status": "node dist/cli/cli.js status",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:linux": "npm run build && electron-builder --linux",
    "package:portable": "npm run build && electron-builder --win portable",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "postinstall": "npm run build",
    "setup": "npm install && npm link"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "node-pty": "^1.1.0",
    "uuid": "^9.0.0",
    "@xterm/xterm": "^5.5.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-search": "^0.15.0",
    "zustand": "^5.0.10",
    "react-mosaic-component": "^6.1.1",
    "immer": "^10.0.3",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.1.0",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.0",
    "eslint": "^9.39.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^16.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.53.0",
    "vite": "^6.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vitest": "^2.1.0"
  },
  "build": {
    "appId": "com.agenteck.app",
    "productName": "Agenteck",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "bin/",
        "to": "bin/",
        "filter": ["**/*"]
      }
    ],
    "win": {
      "target": ["nsis", "portable"],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "assets/icon.icns",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "assets/icon.png",
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Agenteck"
    },
    "portable": {
      "artifactName": "Agenteck-${version}-portable.exe"
    }
  }
}
